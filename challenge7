#include<stdio.h>
#include<string.h>
	struct Student
	{
		char student_ID[17];
		int seat1;
		int seat2;
	}student[1000];
	
int main()
{
	struct Student stduent[1000] ;
	int Iint[1000];//存放试机座位号 
	int i,j,n,a;
	printf("考生数:\n");
	scanf("%d",&a);
	for(i=0;i<a;i++)
	{
		scanf("%s %d %d",&student[i].student_ID,&student[i].seat1,&student[i].seat2);
	}
	printf("迟到考生数:");
	scanf("%d",&n);//n:有多少考生迟到 
	for(j=0;j<n;j++)
	{
		printf("迟到考生的座位号:");
		scanf("%d",&Iint[j]);//输入迟到考生的试机座位号 
		for(i=0;i<a;i++)//匹配试机座位号 
		{
			if(Iint[j]==student[i].seat1)
			{
				printf("考号:%s 考试座位号:%d\n",student[i].student_ID,student[i].seat2);
				break;
			}
		}
	}
	return 0;
}
